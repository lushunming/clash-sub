mixed-port: 7890
allow-lan: true
bind-address: '*'
mode: rule
log-level: info
external-controller: '127.0.0.1:9090'

#订阅节点（自建节点和机场节点引用），更新订阅可以在clash UI代理下的代理集合手动更新
#输入自己的订阅名称和订阅对应的存储在本地名字，ulr后输入订阅地址（别忘了修改第二部分策略组对应引用的订阅名称）
#可用正则剔除高倍节点和一些乱七八糟的节点（比如什么机场网址、套餐到期时间什么的）
proxy-providers:
  订阅A:
    type: http
    path: ./profiles/proxies/订阅A.yaml
    url: https://gh-proxy.com/raw.githubusercontent.com/Ruk1ng001/freeSub/main/clash.yaml
    health-check:
      enable: true
      url: http://cp.cloudflare.com/generate_204
      interval: 300
#  filter: 此处可以按需根据正则筛选节点

  订阅B:
    type: http
    path: ./profiles/proxies/订阅B.yaml
    url: https://ghproxy.net/https://raw.githubusercontent.com/peasoft/NoMoreWalls/master/list.meta.yml
    health-check:
      enable: true
      url: http://cp.cloudflare.com/generate_204
      interval: 300

  订阅c:
    type: http
    path: ./profiles/proxies/订阅c.yaml
    url: https://raw.kkgithub.com/ripaojiedian/freenode/main/clash
    health-check:
      enable: true
      url: http://cp.cloudflare.com/generate_204
      interval: 300
  订阅d:
    type: http
    path: ./profiles/proxies/订阅d.yaml
    url: https://ghfile.geekertao.top/https://raw.githubusercontent.com/PuddinCat/BestClash/main/proxies.yaml
    health-check:
      enable: true
      url: http://cp.cloudflare.com/generate_204
      interval: 300
#策略组
#我常用的AI、电报、YouTube分流对应的策略，香港、日本、新加坡、美国  按地区策略组
proxy-groups:
#PROXY为默认代理组，香港、日本、新加坡节点组负载均衡
  - name: PROXY
    type: select
    proxies:
      - auto
      - 香港
      - 日本
      - 新加坡
      - 美国
    url: http://cp.cloudflare.com/generate_204
    interval: 300    
      
      
#AI的分流都走这个，我一般选美国节点
  - name: AI
    type: select
    proxies:
      - 美国
      - 新加坡

  - name: 电报
    type: select
    proxies:
      - PROXY
      - 香港
      - 新加坡

  - name: Youtube
    type: select
    proxies:
      - PROXY
      - 香港
      - 新加坡
      - 日本
      - 美国

  - name: Google
    type: select
    proxies:
      - PROXY
      - 香港
      - 美国

  - name: 微软
    type: select
    proxies:
      - PROXY
      - 香港
      - 美国

#香港、日本、新加坡、美国  节点按地区策略分组，地区节点组按延迟自动选择
  - name: auto
    type: url-test
    url: http://cp.cloudflare.com/generate_204
    interval: 300
    lazy: true
    timeout: 5000
    max-failed-times: 5
    tolerance: 30
    use:
      - 订阅A
      - 订阅B
      - 订阅c
      - 订阅d
  - name: 香港
    type: url-test
    url: http://cp.cloudflare.com/generate_204
    interval: 300
    lazy: true
    timeout: 5000
    max-failed-times: 5
    tolerance: 30
    use:
      - 订阅A
      - 订阅B
      - 订阅c
      - 订阅d

    filter: 香港|HK|Hong Kong

  - name: 日本
    type: url-test
    url: http://cp.cloudflare.com/generate_204
    interval: 300
    lazy: true
    timeout: 5000
    max-failed-times: 5
    tolerance: 30
    use:
      - 订阅A
      - 订阅B
      - 订阅c
      - 订阅d
    filter: 日本|JP|Japan

  - name: 新加坡
    type: url-test
    url: http://cp.cloudflare.com/generate_204
    interval: 300
    lazy: true
    timeout: 5000
    max-failed-times: 5
    tolerance: 30
    use:
      - 订阅A
      - 订阅B
      - 订阅c
      - 订阅d
    filter: 新加坡|狮城|SG|Singapore

  - name: 美国
    type: url-test
    url: http://cp.cloudflare.com/generate_204
    interval: 300
    lazy: true
    timeout: 5000
    max-failed-times: 5
    tolerance: 30
    use:
      - 订阅A
      - 订阅B
      - 订阅c
      - 订阅d
    filter: 美国|US|United States

#兜底策略组
  - name: 终则
    type: select
    proxies:
      - PROXY
      - DIRECT


